#include<stdio.h>
void main()
{
    int M1,M2,M3,sum=0;
    float per;
    printf("Enter the marks of the student: ");
    scanf("%d%d%d",&M1,&M2,&M3);
    sum=M1+M2+M3;
    per=sum*100/300;
    printf("Percentage of student is %f\n",per);
    if(per>=85 && per<=100)
    {
        printf("The student got Grade A\n");
    }
    else if(per>=70 && per<=84)
    {
        printf("The student got Grade B\n");
    }
    else if(per>=55 && per<=69)
    {
        printf("The student got Grade C\n");
    }
    else if(per>=40 && per<=54)
    {
        printf("The student got Grade D\n");
    }
    else
        printf("The student got Grade F\n");
}
